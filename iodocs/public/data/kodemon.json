{    
    "basePath": "http://localhost:4001",
    "name": "kodemon",
    "protocol": "rest",
    "publicPath": "/api/v1",
    "version": "1.0",
    "resources": {
        "Identity Methods": {
            "methods": {
                "keys": {
                    "description": "This method returns list of all keys",
                    "httpMethod": "GET",
                    "name": "keys",
                    "parameters": {
                        
                    },
                    "path": "/keys"
                },
                "key": {
                    "description": "This method returns all messages for given key",
                    "httpMethod": "GET",
                    "name": "key",
                    "parameters": {
                        "key": {
                            "default": "test.py-test",
                            "description": "A key identity",
                            "location": "path",
                            "required": true,
                            "type": "string"
                        }
                    },
                    "path": "/key/{key}"
                },
                "KeyFromTake": {
                    "description": "This method allows you to retrieve fixed amount of messages for given key from given position",
                    "httpMethod": "GET",
                    "name": "keyfromtake",
                    "parameters": {
                        "key": {
                            "default": "test.py-test",
                            "description": "A key identity",
                            "required": true,
                            "type": "string"
                        },
                        "from" : {
                        	"default": "1",
                            "description": "Start position for from",
                            "required": true,
                            "type": "string"	
                        },
                        "take" : {
                        	"default": "5",
                            "description": "Amount to take",
                            "required": true,
                            "type": "string"	
                        }
                    },
                    "path": "/key/{key}/from/{from}/take/{take}"
                },
                "Count": {
                    "description": "This method returns messages count for key",
                    "httpMethod": "GET",
                    "name": "count",
                    "parameters": {
                        "key": {
                            "default": "test.py-test",
                            "description": "A key identity",
                            "location": "path",
                            "required": true,
                            "type": "string"
                        }
                    },
                    "path": "/key/{key}/count"
                },
                "GetLastNr": {
                    "description": "This method returns last messages for key with given number",
                    "httpMethod": "GET",
                    "name": "getlast",
                    "parameters": {
                        "key": {
                            "default": "test.py-test",
                            "description": "A key identity",
                            "location": "path",
                            "required": true,
                            "type": "string"
                        },
                        "lastNr" : {
                            "default": "5",
                            "description": "Number of messages",
                            "location": "path",
                            "required": true,
                            "type": "string"  
                        }
                    },
                    "path": "/key/{key}/getlast/{lastNr}"
                }
            }
        },
        "User Methods": {
            "methods": {
                "Influence": {
                    "description": "This method allows you to retrieve a user's influencers and influencees.",
                    "httpMethod": "GET",
                    "name": "Influence",
                    "parameters": {
                        "kloutId": {
                            "default": "635263",
                            "description": "A kloutId (like 635263)",
                            "location": "path",
                            "required": true,
                            "type": "string"
                        }
                    },
                    "path": "/user.json/{kloutId}/influence"
                },
                "Score": {
                    "description": "This method allows you to retrieve a user's Klout Score and deltas.",
                    "httpMethod": "GET",
                    "name": "Score",
                    "parameters": {
                        "kloutId": {
                            "default": "635263",
                            "description": "A kloutId (like 635263)",
                            "location": "path",
                            "required": true,
                            "type": "string"
                        }
                    },
                    "path": "/user.json/{kloutId}/score"
                },
                "ShowUser": {
                    "description": "This method allows you to retrieve a user.",
                    "httpMethod": "GET",
                    "name": "Show User",
                    "parameters": {
                        "kloutId": {
                            "default": "635263",
                            "description": "A kloutId (like 635263)",
                            "location": "path",
                            "required": true,
                            "type": "string"
                        }
                    },
                    "path": "/user.json/{kloutId}"
                },
                "Topics": {
                    "description": "This method allows you to retrieve a user's topics.",
                    "httpMethod": "GET",
                    "name": "Topics",
                    "parameters": {
                        "kloutId": {
                            "default": "635263",
                            "description": "A kloutId (like 635263)",
                            "location": "path",
                            "required": true,
                            "type": "string"
                        }
                    },
                    "path": "/user.json/{kloutId}/topics"
                }
            }
        }
    }
}
